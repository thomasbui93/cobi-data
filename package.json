{
  "name": "@moonage/cobi-data",
  "version": "0.0.4",
  "description": "A data structure library",
  "keywords": [
    "typescript",
    "data structures"
  ],
  "main": "dist/cobi-data.umd.js",
  "module": "dist/cobi-data.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Khoa Bui <buidangkhoa2602@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/thomasbui93/cobi-data.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && npm run doc",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --env=node --runInBand",
    "test:watch": "jest --watch --env=node",
    "test:prod": "npm run prettier-format && npm run test -- --coverage --no-cache",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "check": "npm test && pkg-ok",
    "doc": "typedoc --out docs --target es6 --theme minimal --mode file src"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "colors": "^1.4.0",
    "commitizen": "^4.2.3",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^33.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "lodash.camelcase": "^4.3.0",
    "lodash.shuffle": "^4.2.0",
    "pkg-ok": "^2.3.1",
    "prettier": "^2.2.1",
    "prettier-quick": "0.0.5",
    "pretty-quick": "^3.1.0",
    "prompt": "^1.1.0",
    "replace-in-file": "^6.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4",
    "validate-commit-msg": "^2.12.2"
  },
  "bugs": {
    "url": "https://github.com/thomasbui93/cobi-data/issues"
  },
  "homepage": "https://github.com/thomasbui93/cobi-data#readme",
  "directories": {
    "doc": "docs"
  }
}
