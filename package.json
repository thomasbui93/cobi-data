{
  "name": "@moonage/cobi-data",
  "version": "0.0.1",
  "description": "A data structure library",
  "keywords": [
    "typescript",
    "data structures"
  ],
  "main": "dist/cobi-data.umd.js",
  "module": "dist/cobi-data.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Khoa Bui <buidangkhoa2602@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/thomasbui93/cobi-data.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && npm run doc",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --env=node --runInBand",
    "test:watch": "jest --watch --env=node",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "check": "npm test && pkg-ok",
    "doc": "typedoc --out docs --target es6 --theme minimal --mode file src"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/lodash": "^4.14.158",
    "@types/node": "^12.12.53",
    "colors": "^1.4.0",
    "commitizen": "^4.1.2",
    "coveralls": "^3.1.0",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.19",
    "lodash.camelcase": "^4.3.0",
    "lodash.shuffle": "^4.2.0",
    "pkg-ok": "^2.3.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^4.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.23.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.24.3",
    "semantic-release": "^15.14.0",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.15.8",
    "typescript": "^3.9.7",
    "validate-commit-msg": "^2.12.2"
  },
  "bugs": {
    "url": "https://github.com/thomasbui93/cobi-data/issues"
  },
  "homepage": "https://github.com/thomasbui93/cobi-data#readme",
  "directories": {
    "doc": "docs"
  }
}
