{
  "name": "@moonage/cobi-data",
  "version": "0.0.1",
  "description": "A data structure library",
  "keywords": [
    "typescript",
    "data structures"
  ],
  "main": "dist/cobi-data.umd.js",
  "module": "dist/cobi-data.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Khoa Bui <buidangkhoa2602@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/thomasbui93/cobi-data.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --env=node",
    "test:watch": "jest --watch --env=node",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "check": "npm test && pkg-ok && npm run prettier",
    "prettier": "prettier --write \"{src,__tests__}/**/*.ts\""
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "silent": false,
    "verbose": false,
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/",
      "index.ts"
    ],
    "collectCoverageFrom": [
      "src/lib/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "testURL": "https://www.google.com"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.116",
    "@types/node": "^12.7.10",
    "colors": "^1.1.2",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.0",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.0.8",
    "jest": "^25.0.0",
    "lint-staged": "^9.4.1",
    "lodash.camelcase": "^4.3.0",
    "pkg-ok": "^2.3.1",
    "prettier": "^1.13.4",
    "prompt": "^1.0.0",
    "replace-in-file": "^4.1.3",
    "rimraf": "^3.0.0",
    "rollup": "^1.22.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.24.3",
    "semantic-release": "^15.13.25",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.15.0",
    "typescript": "^3.4",
    "validate-commit-msg": "^2.12.2"
  },
  "dependencies": {
    "lodash": "^4.17.13"
  },
  "bugs": {
    "url": "https://github.com/thomasbui93/cobi-data/issues"
  },
  "homepage": "https://github.com/thomasbui93/cobi-data#readme",
  "directories": {
    "doc": "docs"
  }
}
