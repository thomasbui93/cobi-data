{
  "name": "@moonage/cobi-data",
  "version": "0.0.1",
  "description": "A data structure library",
  "keywords": [
    "typescript",
    "data structures"
  ],
  "main": "dist/cobi-data.umd.js",
  "module": "dist/cobi-data.es5.js",
  "typings": "dist/types/cobi-data.d.ts",
  "files": [
    "dist"
  ],
  "author": "Khoa Bui <buidangkhoa2602@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/thomasbui93/cobi-data.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --env=node",
    "test:watch": "jest --watch --env=node",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "{src,__tests__}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "jest": {
    "silent": false,
    "verbose": false,
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/",
      "index.ts"
    ],
    "collectCoverageFrom": [
      "src/lib/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "testURL": "https://www.google.com"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@types/jest": "^22.0.0",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.0.3",
    "colors": "^1.1.2",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.14.0",
    "jest": "^25.0.0",
    "lint-staged": "^7.1.3",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.13.4",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.0.0-beta.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.59.2",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.11.1",
    "semantic-release": "^15.13.25",
    "ts-jest": "^24.1.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.4",
    "validate-commit-msg": "^2.12.2"
  },
  "dependencies": {
    "lodash": "^4.17.13"
  },
  "bugs": {
    "url": "https://github.com/thomasbui93/cobi-data/issues"
  },
  "homepage": "https://github.com/thomasbui93/cobi-data#readme",
  "directories": {
    "doc": "docs"
  }
}
